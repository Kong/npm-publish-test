{
  "name": "@kong-ui-public/npm-publish-test",
  "version": "1.0.0",
  "description": "Test trusted publishing to npm registry",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kong/npm-publish-test.git"
  },
  "homepage": "https://github.com/Kong/npm-publish-test/",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "files": [
    "dist"
  ],
  "types": "dist/types/index.d.ts",
  "typings": "dist/types",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./package.json": "./package.json",
    "./dist/*": "./dist/*"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm typecheck && vite build && vue-tsc -p './tsconfig.build.json' --emitDeclarationOnly && tsc-alias -p './tsconfig.build.json'",
    "commit": "cz",
    "typecheck": "vue-tsc -p './tsconfig.build.json' --noEmit",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "semantic-release": "semantic-release"
  },
  "peerDependencies": {
    "vue": ">= 3.3.13 < 4"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/types": "^7.28.2",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@digitalroute/cz-conventional-changelog-for-jira": "^8.0.1",
    "@evilmartians/lefthook": "^1.12.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@stylistic/stylelint-plugin": "^4.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/json-schema": "^7.0.15",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^22.17.0",
    "@types/sanitize-html": "^2.16.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "c8": "^10.1.3",
    "commitizen": "^4.3.1",
    "cross-env": "^10.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.32.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "postcss-html": "^1.8.0",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^6.0.3",
    "sass": "^1.90.0",
    "sass-embedded": "^1.90.0",
    "semantic-release": "^24.2.7",
    "stylelint": "^16.23.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recommended-scss": "^15.0.1",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-order": "^7.0.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "vite": "^7.0.6",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vite-plugin-replace": "^0.1.1",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vitest": "3.2.4",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1",
    "vue-tsc": "^3.0.5"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@evilmartians/lefthook",
      "@parcel/watcher",
      "esbuild",
      "vue-demi"
    ]
  },
  "author": {
    "name": "Kong Inc.",
    "url": "https://konghq.com/"
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      "next",
      "next-major",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ]
        }
      ],
      "@semantic-release/github"
    ]
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "volta": {
    "node": "24.6.0",
    "pnpm": "10.14.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@digitalroute/cz-conventional-changelog-for-jira",
      "skipScope": false,
      "jiraOptional": true,
      "jiraLocation": "post-description",
      "jiraPrepend": "[",
      "jiraAppend": "]"
    }
  }
}
